{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory Name",
            "defaultValue": "wc-adf-test"
        },
        "LinkedServiceCosas_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'LinkedServiceCosas'"
        },
        "LinkedServiceCosas_password": {
            "type": "secureString",
            "metadata": "Secure string for 'password' of 'LinkedServiceCosas'"
        },
        "LinkedServiceCosasBlob_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'LinkedServiceCosasBlob'"
        },
        "DestinationDataset_cfi_properties_typeProperties_fileName": {
            "type": "string",
            "defaultValue": "@dataset().cw_fileName"
        },
        "DestinationDataset_cfi_properties_typeProperties_folderPath": {
            "type": "string",
            "defaultValue": "adfstagedcopytempdata"
        },
        "LinkedServiceCosas_properties_typeProperties_userName": {
            "type": "string",
            "defaultValue": "RABODEVEU\\Eijndhovenc"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas Token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "LinkedServiceCosasBlob_connectionString": {
                        "value": "[parameters('LinkedServiceCosasBlob_connectionString')]"
                    },
                    "LinkedServiceCosas_connectionString": {
                        "value": "[parameters('LinkedServiceCosas_connectionString')]"
                    },
                    "LinkedServiceCosas_password": {
                        "value": "[parameters('LinkedServiceCosas_password')]"
                    },
                    "DestinationDataset_cfi_properties_typeProperties_fileName": {
                        "value": "[parameters('DestinationDataset_cfi_properties_typeProperties_fileName')]"
                    },
                    "DestinationDataset_cfi_properties_typeProperties_folderPath": {
                        "value": "[parameters('DestinationDataset_cfi_properties_typeProperties_folderPath')]"
                    },
                    "LinkedServiceCosas_properties_typeProperties_userName": {
                        "value": "[parameters('LinkedServiceCosas_properties_typeProperties_userName')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}